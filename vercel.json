{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": ["BaseProject/**/**", "HomepageBuilder/**/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "installCommand": "pip install -r requirements.txt --target /tmp"
}